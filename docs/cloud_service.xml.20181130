				SELECT  A.CTGRY_CL_CD 
				  FROM  TST_CATE_MGMT_M A
				      , TST_GOOD_CATE_I B
				      , TST_GOOD_INFO_I C
				 WHERE  A.PARNTS_CTGRY_CODE >=  '0'
