		<Sub order="16" type="subQuery" seperator="||" wherecnt="1" className="0x00">
			<sql statement="n">
      	<![CDATA[
					SELECT	C.FILE_URL
						FROM  TST_GOOD_MARK_I A, TST_GOOD_MARK_M B, OP_FILE C
					 WHERE  A.MARK_CD = B.MARK_CD
						 AND  B.mark_image_file_seq = C.file_seq
