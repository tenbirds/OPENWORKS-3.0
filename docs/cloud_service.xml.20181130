    <Static>
        <Select>
            <sql>
            <![CDATA[
          	SELECT  DOCID
									, REGISTDT
									, LANGCODE
									, GOODSNM
									, USERID
									, GOODSIMAGEFILESEQ
									, GOODSSUMRY
									, GOODSRDCNT
									, CNTCINSTTCD
									, LANGNM
									, SLEMTHDNM
									, GOODSREGISTSTTUSNM
									, SELLCNT
									, EVLSCORE
									, LANGSTORENM
									, CMMNTYID
									, GOODS_CODE
									, GOODS_CODE
									, GOODS_CODE
									, GOODS_CODE
									, GOODS_CODE
									, GOODS_CODE
									, GOODS_CODE
									, GOODS_CODE
									, GOODS_CODE
									, GOODS_CODE
									, GOODS_CODE
									, GOODS_CODE
									, GOODS_CODE
									, GOODS_CODE
									, GOODS_CODE
									, CATE_NM
									, GOODSMAINIMAGE
									, CTGRY_CODE
									, LANG_CTGRY_NM
									, CTGRY_CL_CD
									, CTGRY_GROUP
									, CTGRY_DP
									, PARNTS_CTGRY_CODE
									, SUB_CNT
						  FROM  (SELECT  CTGRY_CODE
													 , LANG_CTGRY_NM
													 , CTGRY_CL_CD
													 , CTGRY_GROUP
													 , CTGRY_DP
													 , PARNTS_CTGRY_CODE
													 , SUB_CNT
													 , GOODS_CODE  AS DOCID
													 , LANG_CODE AS LANGCODE
													 , goodsNm AS GOODSNM
													 , userId AS USERID 
													 , goodsImageFileSeq AS GOODSIMAGEFILESEQ
													 , goodsSumry AS GOODSSUMRY
													 , goodsRdcnt AS GOODSRDCNT
													 , CNTC_INSTT_CD AS CNTCINSTTCD 
													 , LANGNM
													 , SLEMTHDNM
													 , GOODSREGISTSTTUSNM
													 , SELLCNT
													 , EVLSCORE
													 , LANGSTORENM
													 , CMMNTYID
													 , GOODS_CODE
													 , REGIST_DT
													 , CONFM_DT
													 , TO_CHAR(A.REGIST_DT,'YYYYMMDDHH24MISS') AS REGISTDT
													 , cntrctCnt
													 , SMALL_BUSINESS_CHK
													 , ENTNM //회사명
													 , CATE_NM
													 , GOODSMAINIMAGE /*상품메인이미지*/
											 FROM  (SELECT  DECODE(A.CTGRY_CODE, '6202_343', '6202_344', '6202_349', '6202_350', '6202_341', '6202_350', '6202_351', '6202_348', '6202_337', '6202_353', A.CTGRY_CODE) AS CTGRY_CODE
																		, DECODE(A.CTGRY_CODE, '6202_343', '마케팅/세일즈', '6202_350', '산업', '6202_349', '산업', '6202_341', '산업', '6202_348', '교육/도서', '6202_351', '교육/도서', '6202_353', '방송/문화/콘텐츠', '6202_337', '방송/문화/콘텐츠', A.LANG_CTGRY_NM) AS LANG_CTGRY_NM
																		, A.CTGRY_CL_CD
																		, A.CTGRY_GROUP
																		, A.CTGRY_DP
																		, A.PARNTS_CTGRY_CODE
																		, 0 AS SUB_CNT
																		, B.CTGRY_CODE AS CTGRYCODE
																		, C.GOODS_CODE
																		, C.LANG_CODE
																		, C.GOODS_NM AS goodsNm
																		, C.USER_ID AS userId
																		, C.GOODS_IMAGE_FILE_SEQ AS goodsImageFileSeq
																		, C.GOODS_SUMRY AS goodsSumry
																		, C.GOODS_RDCNT AS goodsRdcnt
																		, C.CNTC_INSTT_CD
																		, (SELECT LANG_NM FROM TCO_SVC_LANG_C WHERE LANG_CODE = A.LANG_CODE) AS LANGNM
																		, (SELECT INDVDLZ_CD_NM FROM OP_CODE_INDVDLZ WHERE LANG_CODE=A.LANG_CODE AND GROUP_CD=1007 AND INDVDLZ_CD = C.SLE_MTHD_CD) AS SLEMTHDNM
																		, (SELECT INDVDLZ_CD_NM FROM OP_CODE_INDVDLZ WHERE LANG_CODE=A.LANG_CODE AND GROUP_CD=1008 AND INDVDLZ_CD = C.GOODS_REGIST_STTUS) AS GOODSREGISTSTTUSNM
																		, (SELECT COUNT(GOODS_CODE) FROM TST_GOOD_ORDR_I WHERE GOODS_CODE = B.GOODS_CODE) AS SELLCNT
																		, (SELECT TO_CHAR(AVG(EVL_SCORE), '9.9') FROM TST_GOOD_EVL_I WHERE GOODS_CODE = B.GOODS_CODE) AS EVLSCORE
																		, (SELECT LANG_STORE_NM FROM TUM_SVC_STOR_I WHERE USER_ID = C.USER_ID AND LANG_CODE = A.LANG_CODE) AS LANGSTORENM
																		, (SELECT CMMNTY_ID FROM TCM_CMNT_INFO_I WHERE MNGR_CLS_AT = 'N' AND CMMNTY_STTUS_CD = '1002' AND LANG_CODE = A.LANG_CODE AND USER_ID = C.USER_ID ORDER BY CONFM_DT DESC LIMIT 1) AS CMMNTYID     
																		, C.REGIST_DT
																		, C.CONFM_DT
																		, C.USER_ID
																		, C.SMLPZ_CHK
																		, (SELECT COUNT(*) FROM TCM_CNTRCT_MANAGE_I WHERE GOODS_CODE = C.GOODS_CODE) AS cntrctCnt
																		, DECODE(C.SMLPZ_CHK,'Y','on','') AS SMALL_BUSINESS_CHK  //중소기업 여부 체크 (Y, N)    
																		, (SELECT cmpny_nm FROM op_user_entrprs_option WHERE USER_ID = C.USER_ID) AS ENTNM //회사명
																		, lang_ctgry_nm AS CATE_NM
																		, (SELECT FILE_URL FROM OP_FILE WHERE FILE_SEQ = C.GOODS_IMAGE_FILE_SEQ ORDER BY FILE_ORDR_NO ASC LIMIT 1) AS GOODSMAINIMAGE /*상품메인이미지*/
													      FROM  TST_CATE_MGMT_M A, TST_GOOD_CATE_I B, TST_GOOD_INFO_I C
															 WHERE  A.PARNTS_CTGRY_CODE >=  '0'
															   AND  A.LANG_CODE = '00'
															   AND  A.CTGRY_DELETE_AT = 'N'
															   AND  A.CTGRY_USE_YN = 'Y'
															   AND  B.GOODS_CODE = C.GOODS_CODE
															   AND  C.MNGR_DELETE_AT = 'N'
															   AND  C.GOODS_ACTVTY_AT = 'Y'
															   AND  C.GOODS_REGIST_STTUS = 1006
															   AND  C.LANG_CODE =  '00'
															   AND A.CTGRY_CL_CD IN ('1001', '1002', '1003', '1004')	 /* 카테고리구분 [ex) PaaS:1001, SaaS:1002, IaaS : 1003, 	SCS : 1004]*/								
															   AND  DECODE(A.CTGRY_CODE, '6202_368', '6202_360', A.CTGRY_CODE)  = B.CTGRY_CODE
					                    )  AS A
				                  WHERE  1=1
				                  GROUP  BY DOCID,GOODSNM
			              ) Y
            ]]>
            </sql>
        </Select>
    </Static>
    <PrimaryKey>
        <Column name="DOCID"/>
    </PrimaryKey>
</Source>
