			<!-- 공공기관 이용 필터 (국내 데이터센터 위치, 나라장터종합쇼핑몰 서비스): META2 -->
		<Sub order="18" type="subQuery" seperator="||" wherecnt="1" className="0x00">
    	<sql statement="n">
      	<![CDATA[
						SELECT CAST(A.GROUP_CD AS  VARCHAR) + CAST(A.INDVDLZ_CD AS  VARCHAR)
						  FROM TST_GOOD_META_I A, OP_CODE_INDVDLZ B 
						 WHERE 1=1 
							 AND A.GROUP_CD = B.GROUP_CD 
							 AND A.INDVDLZ_CD = B.INDVDLZ_CD 
							 AND A.LANG_CD = B.LANG_CODE
							 AND B.LANG_CODE = '00' 
							 AND B.USE_YN = 'Y'
							 AND CAST(A.GROUP_CD AS  VARCHAR) = '2060'
							 AND A.GOODS_CODE = ?
						 GROUP BY A.GOODS_CODE
				]]>
      </sql>
    </Sub>
      
