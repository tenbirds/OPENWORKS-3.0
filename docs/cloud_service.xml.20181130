				   AND  C.GOODS_REGIST_STTUS = 1006
				   AND  C.LANG_CODE =  '00'
				   AND  DECODE(A.CTGRY_CODE, '6202_368', '6202_360', A.CTGRY_CODE)  = B.CTGRY_CODE
				   AND  C.GOODS_CODE = ?
