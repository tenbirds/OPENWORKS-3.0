<?xml version="1.0" encoding="UTF-8"?>
<!-- Application context definition for PetClinic Datasource. -->
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<!-- ========================= VIEW DEFINITION ========================= -->

	<bean id="jsonView" class="zes.base.spring.view.JsonView" />
    <bean id="textView" class="zes.base.spring.view.TextView" />
    <bean id="htmlView" class="zes.base.spring.view.HtmlView" />
	<bean id="downloadView" class="zes.base.spring.view.DownloadView" />
	<bean id="pdfView" class="zes.base.spring.view.PdfView" />
	<bean id="atomView" class="zes.base.spring.view.AtomView" />
	<bean id="rssView" class="zes.base.spring.view.RssView" />
	<bean id="excelView" class="zes.base.spring.view.ExcelView" />

</beans>