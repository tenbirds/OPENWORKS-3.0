    <locations>
        <!-- web.xml 파일내의 webAppRootKey 키의 값을 설정(WAS 서버의 루트 경로 정보를 획득시 사용됨) -->
        <webAppRootKey>openworks3.root</webAppRootKey>

        <!-- was 서버 root 경로 설정 : 운영시 값을 삭제하여 빈값으로 만듬 -->
        <webapproot></webapproot>

        <!-- 첨부파일 저장 위치 : 값이 없는 경우 was 서버 root 경로를 자동으로 사용 -->
        <uploadRoot></uploadRoot>

        <!-- 정적컨텐츠 저장 위치 : 값이 없는 경우 was 서버 root 경로를 자동으로 사용 -->
        <resourceRoot></resourceRoot>

        <!-- 일반 첨부 저장 최상위 폴더명 -->
        <uploadFolderPath>upload</uploadFolderPath>

        <!-- 정적컨텐츠 저장 최상위 경로(웹서버에서 직접 접근) -->
        <resourceFolderPath>webcontent</resourceFolderPath>

        <!-- 프로그램별 별도 폴더 미 지정시 사용되는 폴더명 -->
        <defaultUploadFolderPath>default</defaultUploadFolderPath>

        <!-- taglib에서 사용되는 view 화면 jsp 위치 -->
        <includeTaglibRoot>/WEB-INF/pages/common/taglib/</includeTaglibRoot>
        
        <!-- 표준연계 xml 저장경로 -->
        <!-- <g2bXmlUrl>D:/TEST_XML/NEW_XML/</g2bXmlUrl> -->
        <!-- 개발서버 : <g2bXmlUrl>/root/workspace/GePSHome/g2bAPI/fileRep/receive/append/</g2bXmlUrl> -->
        
        <g2bXmlAfter>/home/ompwausrjb/GePSClientAPI/g2bAPI/fileRep/receive/append/</g2bXmlAfter>
        <g2bXmlUrl>/home/ompwausrjb/GePSClientAPI/g2bAPI/fileRep/receive/append/</g2bXmlUrl>
        <g2bXmlRealApply>/home/ompwausrjb/GePSClientAPI/g2bAPI/fileRep/receive/real-apply/</g2bXmlRealApply>
		
        <!-- 표준연계 xml BackUp저장경로 -->
        <!-- <g2bXmlBak>D:/TEST_XML/BAK_XML/</g2bXmlBak> -->
        <!-- 개발서버 : <g2bXmlBak>/root/workspace/GePSHome/g2bAPI/fileRep/receive/backup/</g2bXmlBak> -->
        <g2bXmlBak>/home/ompwausrjb/GePSClientAPI/g2bAPI/fileRep/receive/backup/</g2bXmlBak>
        <g2bXmlLocal>D:/ceart/</g2bXmlLocal>        
        <g2bXmlLocal2>D:/ceart1/</g2bXmlLocal2>        
                
