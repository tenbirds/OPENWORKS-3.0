	<overseasProducts>
		<!-- 해외동양 서비스 가져오기 모니터링 Url -->
		<digitalMarketplace>
			<protocol>https</protocol>
			<domain>www.digitalmarketplace.service.gov.uk</domain>
			<url>/g-cloud/search</url>
			<queryString>q=</queryString>
			<charset>utf-8</charset>
		</digitalMarketplace>
	</overseasProducts>

	<autoMail>
        <senderName>OpenMarketplace</senderName>
        <senderEmail>webmaster@ceart.kr</senderEmail>
