        <mgrUrlPrefix>/intra/</mgrUrlPrefix>
        <!-- 사용자 URL Prefix -->
        <userUrlPrefix>/web/</userUrlPrefix>
		<!-- 모바일 URL Prefix -->
		<mobileUrlPrefix>/m/</mobileUrlPrefix>
		<!-- board 모바일 URL Prefix -->
		<boardMobileUrlPrefix>/mobile/</boardMobileUrlPrefix>
        <!-- 외국어사용자 URL Prefix -->
		<portalUrlPrefix>/pt/</portalUrlPrefix>
		<!-- 커뮤니티 URL Prefix -->
		<cmUrlPrefix>/cm/</cmUrlPrefix>
		
        <!-- 관리자 로그인 -->
        <mgrLogin>/login/mgr/ND_index.do</mgrLogin>
        <mgrLoginAction>/login/mgr/ND_loginAction.do</mgrLoginAction>
        <mgrLogout>/login/mgr/ND_logoutAction.do</mgrLogout>
        <mgrLostSession>/login/mgr/ND_LostSession.do</mgrLostSession>
        <!-- 포털 로그인 -->
        <userLogin>/korean/pt/login/user/BD_index.do</userLogin>
        <userLoginAction>/korean/pt/login/user/ND_loginAction.do</userLoginAction>
        <userLogout>/korean/pt/login/user/ND_logoutAction.do</userLogout>
        <userLostSession>/korean/pt/login/user/ND_LostSession.do</userLostSession>
		<!-- 외국언어별 포털 로그인페이지 -->
        <userEngLogin>/english/pt/login/user/BD_index.do</userEngLogin>
		<userEspLogin>/spanish/pt/login/user/BD_index.do</userEspLogin>
		<!-- 모바일언어별 모바일 로그인페이지 -->
		<userEngLoginM>/english/m/login/user/BD_index.do</userEngLoginM>
		<userEspLoginM>/spanish/m/login/user/BD_index.do</userEspLoginM>

        <!-- 관리자 메인페이지 -->
        <mgrMain>/intra/explorer/ND_index.do</mgrMain>
        <!-- 포털 메인페이지 -->
        <userMain>/korean/pt/index.do</userMain>
        <!-- 영어포털 사용자 메인페이지 -->
        <userEngMain>/english/pt/index.do</userEngMain>
        <!-- 스페인어포털 사용자 메인페이지 -->
        <userEspMain>/spanish/pt/index.do</userEspMain>
        <!-- 모바일 사용자 메인페이지 -->
        <mobileMain>/korean/m/index.do</mobileMain>
        <!-- 영어모바일 사용자 메인페이지 -->
        <mobileEngMain>/english/m/index.do</mobileEngMain>
        <!-- 스페인어모바일 사용자 메인페이지 -->
        <mobileEspMain>/spanish/m/index.do</mobileEspMain>
    </url>

