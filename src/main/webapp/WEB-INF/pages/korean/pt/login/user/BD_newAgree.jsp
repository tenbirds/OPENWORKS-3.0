	                    		top.location.href = "http://"+$("#rtnDomain").val() + CTX_PATH+"/korean/pt/register/BD_userUpdateForm.do?pwdChg=Y";
		                    <% } %>
	                    }
	                }else if(responses[0] == 2001){
