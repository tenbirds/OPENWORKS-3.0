		var num1 = 0;
		var jsStplatAgreAction = function() {
			 
		    if($("input:checkbox[id='stplat1']").is(":checked") == false) {
	            alert(Message.msg.agreeTerms);
	            $("#stplat1").focus();
	            return false;
	        }
