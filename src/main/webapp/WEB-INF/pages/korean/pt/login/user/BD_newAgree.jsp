	            alert(Message.msg.agreeTerms);
	            $("#stplat2").focus();
	            return false;
	        }
		    if($("input:checkbox[id='stplat3']").is(":checked") == false) {
	            alert(Message.msg.agreeTerms);
	            $("#stplat3").focus();
	            return false;
	        }
		    if($("#userId").val() == '') {
	            alert(Message.msg.emptyId);
	            $("#userId").focus();
	            return false;
	        }
	        if($("#userPassword").val() == '') {
	            alert(Message.msg.emptyPwd);
	            $("#userPassword").focus();
	            return false;
	        }
	        if($("#userPassword").val().indexOf('●') > -1){
	    	    alert(Message.msg.wrongPwd);
	    		return false;
	    	}
			
			 num1++;
			$.post("/korean/pt/login/user/ND_stplatAgreAction.do", {
	            userId : $("#userId").val(),
	            userPassword : $("#userPassword").val()
	        }, function(response) {
	            if(isNaN(response) && (response.indexOf('200') < 0)) {
	                if(response.indexOf('Withdraw') > -1){
	                    alert(Message.msg.noUseId);
	                }else if(response.indexOf('notMatchLogin') > -1){
	                    alert(Message.msg.notMatchLogin);
	                }else if(response.indexOf('999')){
	                    alert('오류입니다.');
	                }else{
	                    alert(response);
	                }
	            } else {
	                responses = response.split(" ");
	                if(responses[0] == 2001){
	                    alert('약관동의에 실패했습니다. 관리자에게 문의하세요.');
	                } else if(responses[0] == 2002){
	                    alert('이미 동희한 회원입니다.');
	                }  else if(responses[0] == 200){
	                    //jsStplatAgreActionCloud();
	                    alert('약관동의가 완료되었습니다.');
	                    jsLoginAction();
	                } 
	            }
	        }).error(function(result) {
				//alert('약관동의에 실패했습니다. 관리자에게 문의하세요.');
				if(num1++ <= 15) jsStplatAgreAction();
	        });
	        return false; 
	    };
	    
	    <%-- 
